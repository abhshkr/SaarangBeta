<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhi.saarangbeta">

    <!-- For dialing coordinator directly - used in displayDetails.java -->
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/saarangicon"
        android:label="@string/appLabel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".displayDetails"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".regActivity"
            android:parentActivityName=".displayDetails" />

    </application>

</manifest>